{
  "name": "vue-use-leaflet",
  "version": "0.1.1",
  "description": "Vue composition utilities for leaflet map",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://vueuseleaflet.org",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/vue-use-leaflet.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/vue-use-leaflet/issues"
  },
  "keywords": [
    "vue",
    "vue-use",
    "map",
    "leaflet"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "scripts": {
    "watch": "vite build --watch",
    "build": "npm run type-check && npm run build-only && npm run build-types",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "build-types": "vue-tsc -p tsconfig.dts.json",
    "playground": "vite playground",
    "playground:build": "vite build --base=/playground/ playground",
    "playground:postbuild": "cp -r ./playground/dist ./docs/.vuepress/dist/playground",
    "lint": "eslint . --ext .vue,.js,.ts --fix --ignore-path .gitignore",
    "format": "prettier --write \"{src,playground,.test}/**/*.{js,ts,vue,css,md}\"",
    "test": "vitest",
    "test:2": "vue-demi-switch 2.7 vue2 && vitest run --silent",
    "test:3": "vue-demi-switch 3 && vitest run",
    "test:all": "npm run test:3 && npm run test:2 && vue-demi-switch 3",
    "test:cov": "vitest run --coverage",
    "docs:prepare": "npm run build-types && npm run docs:gen",
    "docs:dev": "npm run docs:prepare && vuepress dev docs",
    "docs:build": "npm run docs:prepare && vuepress build docs && npm run docs:playground",
    "docs:gen": "esno docs/.vuepress/scripts/gen-docs.ts",
    "docs:playground": "npm run playground:build && npm run playground:postbuild",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "peerDependencies": {
    "@types/leaflet": ">=1.9.0",
    "leaflet": ">=1.9.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^10.2.1",
    "@vueuse/math": "^10.2.1",
    "@vueuse/shared": "^10.2.1",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@babel/generator": "^7.22.9",
    "@babel/parser": "^7.22.7",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-angular": "^17.6.7",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/jsdom": "^21.1.1",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^20.4.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.0",
    "@vue/tsconfig": "^0.4.0",
    "@vuepress/plugin-register-components": "2.0.0-beta.66",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "esno": "^0.17.0",
    "fast-glob": "^3.3.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "leaflet": "^1.9.4",
    "lint-staged": "^13.2.3",
    "markdown-yaml-metadata-parser": "^3.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "~5.1.6",
    "vite": "~4.3.9",
    "vitest": "^0.33.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vue-tsc": "^1.8.5",
    "vue2": "npm:vue@^2.7.14",
    "vuepress": "2.0.0-beta.66"
  }
}

{
  "name": "vue-use-leaflet",
  "version": "0.0.7",
  "description": "Vue composition utilities for leaflet map",
  "author": "Nikolay Naumenkov <nikolas.it.web@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nikolaynau/vue-use-leaflet#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nikolaynau/vue-use-leaflet.git"
  },
  "bugs": {
    "url": "https://github.com/nikolaynau/vue-use-leaflet/issues"
  },
  "keywords": [
    "vue",
    "vue-use",
    "map",
    "leaflet"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./*": "./*"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "lint-staged": {
    "*.ts": "prettier --write"
  },
  "scripts": {
    "watch": "vite build --watch",
    "build": "npm run type-check && npm run build-only && npm run build-types",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "build-types": "vue-tsc -p tsconfig.dts.json",
    "playground": "vite playground",
    "lint": "eslint . --ext .vue,.js,.ts,.css --fix --ignore-path .gitignore",
    "format": "prettier --write \"{src,playground,.test}/**/*.{js,ts,vue,css}\"",
    "test": "vitest",
    "test:2": "vue-demi-switch 2.7 vue2 && vitest run --silent",
    "test:3": "vue-demi-switch 3 && vitest run",
    "test:all": "npm run test:3 && npm run test:2 && vue-demi-switch 3",
    "test:cov": "vitest run --coverage",
    "postversion": "npm run push",
    "push": "git push origin master --tags",
    "husky": "husky install"
  },
  "peerDependencies": {
    "leaflet": ">=1.9.0",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "dependencies": {
    "@vueuse/core": "^9.13.0",
    "@vueuse/math": "^9.13.0",
    "@vueuse/shared": "^9.13.0",
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@babel/types": "^7.21.3",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-angular": "^17.4.4",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/leaflet": "^1.9.3",
    "@types/node": "^18.15.3",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitest/coverage-c8": "^0.29.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.36.0",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.3",
    "jsdom": "^21.1.1",
    "leaflet": "^1.9.3",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "typescript": "~5.0.2",
    "vite": "^4.2.0",
    "vitest": "^0.29.3",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue-tsc": "^1.2.0",
    "vue2": "npm:vue@^2.7.10"
  }
}
